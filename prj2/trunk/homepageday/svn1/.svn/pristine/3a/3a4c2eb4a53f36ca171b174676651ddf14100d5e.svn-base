<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="TOP_RIGHT" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eproject2_motor.nutController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <FontAwesomeIconView fill="#dce806" glyphName="CIRCLE" onMouseClicked="#min" wrappingWidth="12.0">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIconView>
      <FontAwesomeIconView fill="#07f523" glyphName="CIRCLE" onMouseClicked="#max">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIconView>
      <FontAwesomeIconView fill="RED" glyphName="CIRCLE" onMouseClicked="#close" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIconView>
   </children>
</HBox>
